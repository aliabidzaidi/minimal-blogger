{
  "name": "minimal-blogger",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "all": "npm-run-all -p electron:serve ng:serve",
    "ng:serve": "ng serve",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:serve-tsc": "tsc index.ts",
    "start": "ng serve",
    "build": "ng build",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "@angular/router": "~9.1.9",
    "@tinymce/tinymce-angular": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.0.0",
    "electron": "^9.0.5",
    "knex": "^0.21.1",
    "ng-zorro-antd": "^9.1.2",
    "ngx-electron": "^2.2.0",
    "ngx-quill": "^12.0.1",
    "ngx-toastr": "^12.1.0",
    "npm-run-all": "^4.1.5",
    "quill": "^1.3.7",
    "rxjs": "~6.5.4",
    "sqlite3": "^5.0.0",
    "tslib": "^1.10.0",
    "wait-on": "^5.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "electron-rebuild": "^1.11.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~3.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "crypto": false,
    "stream": false,
    "http": false,
    "tls": false,
    "zlib": false,
    "https": false,
    "net": false,
    "aws-sdk": false,
    "child_process": false,
    "constants": false
  }
